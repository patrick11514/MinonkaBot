{
    "name": "minonka",
    "version": "1.1.0",
    "description": "Discordbot for League of Legends",
    "main": "build/index.js",
    "dependencies": {
        "@patrick115/endpoints": "1.1.2-d",
        "cli-color": "^2.0.3",
        "discord.js": "^14.14.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "kysely": "^0.26.3",
        "microjob": "^0.7.0",
        "module-alias": "^2.2.3",
        "mysql2": "^3.6.5",
        "node-fetch": "^3.3.2",
        "prettier": "^3.1.0",
        "sharp": "^0.32.6",
        "strip-color": "^0.1.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/cli-color": "^2.0.6",
        "@types/node": "^20.10.0",
        "@types/node-fetch": "^2.6.9",
        "@types/strip-color": "^0.1.2",
        "kysely-codegen": "^0.11.0",
        "tsconfig-paths": "^4.2.0"
    },
    "scripts": {
        "folders": "mkdir -p ./logs",
        "dev": "npm run folders && ts-node-dev -r tsconfig-paths/register --respawn --rs ./src/index.ts",
        "build": "mkdir -p build && npm run folders && tsc",
        "start": "node -r module-alias/register ./build/index.js",
        "clear": "rm -rf build",
        "format": "prettier --write .",
        "registerCommandsDev": "ts-node-dev  -r tsconfig-paths/register ./src/registerCommands.ts",
        "registerCommands": "npm build && node -r module-alias/register ./build/registerCommands.js",
        "genDatabaseSchema": "kysely-codegen --out-file ./src/types/database.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/patrick11514/MinonkaBot.git"
    },
    "keywords": [
        "DiscordJS",
        "LeagueOfLegends",
        "LOL",
        "Discord",
        "TS",
        "JS"
    ],
    "author": "Patrik MintÄ›l",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/patrick11514/MinonkaBot/issues"
    },
    "homepage": "https://github.com/patrick11514/MinonkaBot#readme",
    "_moduleAliases": {
        "$": "./build"
    }
}
